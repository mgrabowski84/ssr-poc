{
  "name": "poc-ssr-loadable",
  "version": "0.0.0",
  "description": "Proof of concept for server side rendering react-loadable components ",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel start:watch build:watch",
    "start:watch": "nodemon --watch ./build/client.js --exec \"babel-node server/index.js\"",
    "build": "webpack --config webpack.config.js",
    "clean": "rimraf ./build",
    "prebuild:watch": "run-s build",
    "build:watch": "chokidar '(src|server)/**/*' -c \"npm run build\""
  },
  "repository": "tbd.git",
  "author": "Micha≈Ç Grabowski",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.2.1",
    "express": "^4.16.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "chokidar-cli": "^1.2.1",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  }
}
